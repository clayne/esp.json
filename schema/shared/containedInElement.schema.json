{
  "$id": "esp.json/shared/containedInElement.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Contained In Element",
  "description": "Describes what records this record can be contained in.",
  "type": "object",
  "properties": {
    "name": {
      "$ref": "name.schema.json"
    },
    "type": {
      "const": "formId"
    },
    "required": {
      "const": true
    },
    "signatures": {
      "type": "array",
      "items": {
        "$ref": "signature.schema.json"
      }
    }
  }
}
