      "additionalProperties": false,
      "properties": {
        "signature": {
          "$ref": "shared/signature.schema.json"
        },
        "conflictType": {
          "$ref": "shared/conflictType.schema.json"
        },
        "required": {
          "$ref": "shared/required.schema.json"
        },
        "defFlags": {
